Knowledge:
	Type:		data_driven;;

Data:

mode:=read {mode from Parameters};
Box1:=read {box1 from Parameters};
Box2:=read {box2 from Parameters};

If (mode = PRODUCE) then
spentTimeOutsideSchoolGenPartner2 := read Exist {Spent time outside school with general-partner2} Where it Occurred Within Past 1 Days;
hasGeneralPartner2:=read Exist {General partner2} Where it Occurred Within Past 1 Days;
generalPartner2:=read Last {General partner2} Where it Occurred Within Past 1 Days;
endif
;;

Priority:		284;;
	Evoke:		;;

Logic:
If (mode = PRODUCE) then

If (spentTimeOutsideSchoolGenPartner2 = False) AND (hasGeneralPartner2  = True) then conclude True;
conclude False;

endif

If (mode = CONSUME) then

If  (Box1=true) then
 CALL storeObs With "Spent time outside school with general-partner2", "Yes";
endif;
If  (Box2=true) then
 CALL storeObs With "Spent time outside school with general-partner2", "No";
endif;


endif
;;

Action:
write ("Did you <b> spend time with || generalPartner2_value  || at school? </b>");
;;

Age_Min: 14 years;;
Age_Max: 16 years;;

